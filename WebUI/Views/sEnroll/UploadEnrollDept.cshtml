@{
    Layout = "~/Views/Shared/_Pop.cshtml";
}
<link href="@Url.Content("~/Content/uploadify/uploadify.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Content/uploadify/swfobject.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Content/uploadify/jquery.uploadify.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Content/Scripts/sEnroll/UploadEnrollDept.js")?v=@(new Random().Next())" type="text/javascript"></script>
<div id="upenrollLaout" class="easyui-layout" data-options="fit:true">
    <div title="基本信息" data-options="region:'north',border:false,height:120,doSize:false" id="northPanel">
        <form id="f_enrollForm" class="easyui-form" method="post">
            <table class="t-form">
                <tr style="display: none;">
                    <td></td>
                    <td>
                        <div id="uploadfileQueue5"></div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <span style="float:right;">校区:</span>
                    </td>
                    <td>
                        <input type="text" id="enrollDeptId" name="enrollDeptId" class="easyui-combotree" data-options="required:true" />
                    </td>
                </tr>
                <tr>

                    <td><span style="float:right;"> 选择文件:</span> </td>
                    <td>
                        <div id="enroll_file"></div>
                    </td>
                    <td><span style="float:right;"> 文件名:</span></td>
                    <td>
                        <input type="text" id="enrollDeptFilePath" name="enrollDeptFilePath" class="easyui-textbox" data-options="required:true,readonly:true" />
                    </td>
                </tr>
            </table>
        </form>
    </div>
    <div title="报名校区" data-options="region:'center',border:false">
        <table id="gridEnrollDeptInfo" data-options="border:false,fit:true"></table>
    </div>
</div>
