@{
    Layout = "~/Views/Shared/_Pop.cshtml";
}
<link href="@Url.Content("~/Content/uploadify/uploadify.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Content/uploadify/swfobject.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Content/uploadify/jquery.uploadify.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Content/Scripts/sEnroll/UploadLeaveYear.js")?v=@(new Random().Next())" type="text/javascript"></script>
<div id="upLeaveYearLayout" class="easyui-layout" data-options="fit:true">
    <div title="基本信息" data-options="region:'north',border:false,height:120,doSize:false" id="northPanel">
        <form id="fupLeaveYearForm" class="easyui-form" method="post">
            <table class="t-form">
                <tr style="display: none;">
                    <td></td>
                    <td>
                        <div id="uploadfileQueue4"></div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <span style="float:right;">报名学校:</span>
                    </td>
                    <td>
                        <input type="text" id="yearDeptId" name="yearDeptId" class="easyui-combotree" data-options="required:true" />
                    </td>
                </tr>
                <tr>

                    <td><span style="float:right;"> 选择文件:</span> </td>
                    <td>
                        <div id="yearfile_upload"></div>
                    </td>
                    <td><span style="float:right;"> 文件名:</span></td>
                    <td>
                        <input type="text" id="yearfilePath" name="yearfilePath" class="easyui-textbox" data-options="required:true,readonly:true" />
                    </td>
                </tr>
            </table>
        </form>
    </div>
    <div title="毕业年份信息" data-options="region:'center',border:false">
        <table id="gridInfo" data-options="border:false,fit:true"></table>
    </div>
</div>
