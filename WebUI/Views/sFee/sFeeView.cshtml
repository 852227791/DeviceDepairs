@{
    Layout = "~/Views/Shared/_Pop.cshtml";
}

<script src="@Url.Content("~/Content/Scripts/sFee/sFeeView.js")?v=@(new Random().Next())" type="text/javascript"></script>

<div id="ViewInfo" class="easyui-tabs" data-options="border:false">
    <div title="交费信息" id="t1">
        <table class="bodytable0">
            <tr>
                <td>收费单位:</td>
                <td colspan="3"><span id="spDept"></span></td>
            </tr>
            <tr>
                <td>凭证号:</td>
                <td><span id="spVoucherNum"></span></td>
                <td>票据号:</td>
                <td><span id="spNoteNum"></span></td>
            </tr>
            <tr>
                <td style="width: 70px">姓名:</td>
                <td><span id="spName"></span></td>
                <td style="width: 70px">身份证号:</td>
                <td><span id="spIDCard"></span></td>
            </tr>
            <tr>
                <td>交费时间:</td>
                <td><span id="spFeeTime"></span></td>
                <td>交费方式:</td>
                <td><span id="spFeeMode"></span></td>
            </tr>
            <tr>
                <td>应交金额:</td>
                <td><span id="spShouldMoney"></span>元</td>
                <td>实交金额:</td>
                <td><span id="spPaidMoney"></span>元</td>
            </tr>
            <tr>
                <td>充抵金额:</td>
                <td><span id="spOffset"></span>元</td>
                <td>被充抵金额:</td>
                <td><span id="spByOffset"></span>元</td>
            </tr>
            <tr>
                <td>优惠金额:</td>
                <td><span id="spDiscountMoney"></span>元</td>
                <td>核销金额:</td>
                <td><span id="spRefundMoney"></span>元</td>
            </tr>
            <tr>
                <td>结账人:</td>
                <td><span id="spAffirmID"></span></td>
                <td>结账时间:</td>
                <td><span id="spAffirmTime"></span></td>
            </tr>
            <tr>
                <td>交费方案：</td>
                <td><span id="spPlanName"></span></td>
                <td>状态:</td>
                <td><span id="spStatus"></span></td>
            </tr>
            <tr>
                <td>收费内容：</td>
                <td colspan="3"><span id="spsFeeContent"></span></td>
            </tr>
            <tr>
                <td>配品：</td>
                <td colspan="3"><span id="spsGive"></span></td>
            </tr>
            <tr>
                <td>说明：</td>
                <td colspan="3"><span id="spExplain"></span></td>
            </tr>
            <tr>
                <td>备注：</td>
                <td colspan="3"><span id="spRemark"></span></td>
            </tr>
        </table>
    </div>
    <div title="收费明细" data-options="closable:false">
        <table id="sfeeOrder" data-options="border:false" style="height:496px"></table>
    </div>
    <div id="t2" title="学生信息" data-options="closable:false,fit:true">
        <div id="cc" class="easyui-layout" style="width:786px;height:490px;">
            <div data-options="region:'north',title:'基础信息',split:true,border:false,collapsible:false" style="height:205px;">
                <table class="bodytable0">
                    <tr>
                        <td style="width: 60px">姓名:</td>
                        <td><span id="spStudentName"></span></td>
                        <td style="width: 60px">身份证号:</td>
                        <td><span id="spStudentIDCard"></span></td>
                    </tr>
                    <tr>
                        <td>性别:</td>
                        <td><span id="spSex"></span></td>
                        <td>联系电话:</td>
                        <td><span id="spMobile"></span></td>
                    </tr>
                    <tr>
                        <td>QQ号:</td>
                        <td><span id="spQQ"></span></td>
                        <td>微信号:</td>
                        <td><span id="spWeChat"></span></td>
                    </tr>

                    <tr>
                        <td>家庭地址：</td>
                        <td colspan="3"><span id="spAddress"></span></td>
                    </tr>
                    <tr>
                        <td>说明：</td>
                        <td colspan="3"><span id="spStudentRemark"></span></td>
                    </tr>

                </table>
            </div>
            <div data-options="region:'center',title:'扩展信息',border:false">
                <table class="bodytable0">
                    <tr>
                        <td style="width: 60px">生源地:</td>
                        <td colspan="3"><span id="spPC"></span></td>
                    </tr>
                    <tr>
                        <td style="width: 60px">民族:</td>
                        <td><span id="spNation"></span></td>
                        <td style="width: 60px">邮编:</td>
                        <td><span id="spZip"></span></td>
                    </tr>
                    <tr>
                        <td>毕业学校：</td>
                        <td colspan="3"><span id="spSchool"></span></td>
                    </tr>
                </table>

            </div>
            <div data-options="region:'south',title:'联系人信息',split:true,border:false,collapsible:false" style="height:140px;">
                <table id="studentContanctgrid" data-options="fit:true,border:false"></table>
            </div>
        </div>
     

    </div>

    <div title="核销信息" data-options="closable:false">
        <table id="refundGrid" data-options="border:false" style="height:496px"></table>
    </div>
</div>