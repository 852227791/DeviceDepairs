@{
    Layout = "~/Views/Shared/_Blank.cshtml";
}
@section head
{
    <script src="@Url.Content("~/Content/Scripts/sOrderAdd/sOrderAddList.js")?v=@(new Random().Next())" type="text/javascript"></script>
}
<div id="infoMain" class="easyui-layout" data-options="fit:true">
    <div title="搜索" data-options="region:'north',border:false,collapsible:false,iconCls:'icon-search'" style="height: 57px;">
        <div id="searchBox" class="easyui-layout" data-options="fit:true">
            <form id="fSearch" class="easyui-form">
                <div data-options="region:'north',border:false">
                    <div class="t-div">
                        <div>名称:</div>
                        <div>
                            <input id="txtName" name="txtName" type="text" class="easyui-textbox" style="width: 100px;" />
                        </div>
                        <div>校区:</div>
                        <div>
                            <input id="treeDept" name="treeDept" type="text" class="easyui-combotree" style="width: 100px;" />
                        </div>
                        <div>年份:</div>
                        <div>
                            <input id="selYear" name="selYear" type="text" class="easyui-combobox" style="width: 100px;" />
                        </div>
                        <div>月份:</div>
                        <div>
                            <input id="selMonth" name="selMonth" type="text" class="easyui-combobox" style="width: 100px;" />
                        </div>
                        @*<div>专业:</div>
                            <div>
                                <input id="txtMajor" name="txtMajor" type="text" class="easyui-textbox" style="width: 100px;" />
                            </div>*@
                        <div style="padding-top: 2px;">
                            <a href="javascript:void(0)" id="btnSearch" class="easyui-linkbutton" iconcls="icon-search">查询</a>
                        </div>
                        <div style="padding-top: 2px;">
                            <a href="javascript:void(0)" id="btnReset" class="easyui-linkbutton" iconcls="icon-undo">重置</a>
                        </div>
                        <div style="padding-top: 2px;">
                            <a href="javascript:void(0)" id="openSearchBox" class="easyui-linkbutton" iconcls="icon-add2">展开高级搜索</a>
                            <a href="javascript:void(0)" id="closeSearchBox" class="easyui-linkbutton" iconcls="icon-clear" style="display: none;">关闭高级搜索</a>
                        </div>
                    </div>
                </div>
                <div data-options="region:'center',border:false">
                    <div class="t-div">
                        <div>缴费方案名称:</div>
                        <div>
                            <input id="txtPlanName" name="txtPlanName" type="text" class="easyui-textbox" style="width: 100px;" />
                        </div>
                        <div>缴费次数名称:</div>
                        <div>
                            <input id="txtNumName" name="txtNumName" type="text" class="easyui-textbox" style="width: 100px;" />
                        </div>
                        <div>创建人:</div>
                        <div>
                            <input id="txtCreateName" name="txtCreateName" type="text" class="easyui-textbox" style="width: 100px;" />
                        </div>
                        <div>创建时间:</div>
                        <div>
                            <input id="txtCreateTimeS" name="txtCreateTimeS" type="text" class="easyui-datebox" style="width: 100px;" />
                        </div>
                        <div>至</div>
                        <div>
                            <input id="txtCreateTimeE" name="txtCreateTimeE" type="text" class="easyui-datebox" style="width: 100px;" />
                        </div>
                        <div>状态:</div>
                        <div>
                            <input id="selStatus" name="selStatus" type="text" class="easyui-combobox" style="width: 100px;" />
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
    <div data-options="region:'center',border:false">
        <table id="grid" data-options="fit:true,border:false"></table>
    </div>
</div>
<div id="sOrderAddMore"></div>
<div id="sOrderAddMore-buttons">
    <a href="javascript:void(0)" class="easyui-linkbutton" id="btnSavesOrderAddMore" iconcls="icon-ok">保存</a>
    <a href="javascript:void(0)" class="easyui-linkbutton" onclick="javascript: $('#sOrderAddMore').dialog('close')" iconcls="icon-cancel">关闭</a>
</div>
<div id="sOrderAddView"></div>
<div id="sOrderAddView-buttons">
    <a href="javascript:void(0)" class="easyui-linkbutton" onclick="javascript: $('#sOrderAddView').dialog('close')" iconcls="icon-cancel">关闭</a>
</div>
<div id="choosesOrderDetail"></div>
<div id="choosesOrderDetail-buttons">
    <a href="javascript:void(0)" class="easyui-linkbutton" id="btnSavesOrderDetail" iconcls="icon-ok">保存</a>
    <a href="javascript:void(0)" class="easyui-linkbutton" onclick="javascript: $('#choosesOrderDetail').dialog('close')" iconcls="icon-cancel">关闭</a>
</div>
<div id="sOrderAddResult"></div>
<div id="sOrderAddResult-buttons">
    <a href="javascript:void(0)" id="btnsOrderAddResult" class="easyui-linkbutton" onclick="javascript: $('#sOrderAddResult').dialog('close')" iconcls="icon-cancel">关闭</a>
</div>