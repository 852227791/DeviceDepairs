@{
    Layout = "~/Views/Shared/_Blank.cshtml";
}
@section head
{
    <script src="@Url.Content("~/Content/Scripts/sOrderGive/sOrderGiveList.js")?v=@(new Random().Next())" type="text/javascript"></script>
}

<div id="infoMain" class="easyui-layout" data-options="fit:true">
    <div title="搜索" data-options="region:'north',border:false,collapsible:false,iconCls:'icon-search'" style="height: 57px;">
        <div id="searchBox" class="easyui-layout" data-options="fit:true">
            <form id="fSearch" class="easyui-form">
                <div data-options="region:'north',border:false">
                    <div class="t-div">
                        <div>校区:</div>
                        <div>
                            <input id="treeDept" name="treeDept" type="text" class="easyui-combotree" style="width: 100px;">
                        </div>
                        <div>学生姓名:</div>
                        <div>
                            <input id="txtStuName" name="txtStuName" type="text" class="easyui-textbox" style="width: 100px;" />
                        </div>
                        <div>学号:</div>
                        <div>
                            <input id="txtEnrollNum" name="txtEnrollNum" type="text" class="easyui-textbox" style="width: 100px;" />
                        </div>
                        <div>身份证号:</div>
                        <div>
                            <input id="txtIDCard" name="txtIDCard" type="text" class="easyui-textbox" style="width: 100px;" />
                        </div>
                        <div>报名时间:</div>
                        <div>
                            <input id="txtSignTimeS" name="txtSignTimeS" type="text" class="easyui-datebox" style="width: 100px;" />
                        </div>
                        <div>至</div>
                        <div>
                            <input id="txtSignTimeE" name="txtSignTimeE" type="text" class="easyui-datebox" style="width: 100px;" />
                        </div>
                        <div style="padding-top: 2px;">
                            <a href="javascript:void(0)" id="btnSearch" class="easyui-linkbutton" iconcls="icon-search">查询</a>
                        </div>
                        <div style="padding-top: 2px;">
                            <a href="javascript:void(0)" id="btnReset" class="easyui-linkbutton" iconcls="icon-undo">重置</a>
                        </div>
                        <div style="padding-top: 2px;">
                            <a href="javascript:void(0)" id="openSearchBox" class="easyui-linkbutton" iconcls="icon-add2">展开高级搜索</a>
                            <a href="javascript:void(0)" id="closeSearchBox" class="easyui-linkbutton" iconcls="icon-clear" style="display: none;">关闭高级搜索</a>
                        </div>
                    </div>
                </div>
                <div data-options="region:'center',border:false">
                    <div class="t-div">
                        <div>学习层次:</div>
                        <div>
                            <input id="selLevel" name="selLevel" type="text" class="easyui-combobox" style="width: 100px;" />
                        </div>
                        <div>专业:</div>
                        <div>
                            <input id="txtMajor" name="txtMajor" type="text" class="easyui-textbox" style="width: 100px;" />
                        </div>
                        <div>年份:</div>
                        <div>
                            <input id="txtYear" name="txtYear" type="text" class="easyui-textbox" style="width: 100px;" />
                        </div>
                        <div>月份:</div>
                        <div>
                            <input id="txtMonth" name="txtMonth" type="text" class="easyui-textbox" style="width: 100px;" />
                        </div>
                        <div>缴费方案名称:</div>
                        <div>
                            <input id="txtPlanName" name="txtPlanName" type="text" class="easyui-textbox" style="width: 100px;" />
                        </div>
                        <div>配品名称:</div>
                        <div>
                            <input id="txtGiveName" name="txtGiveName" type="text" class="easyui-textbox" style="width: 100px;" />
                        </div>
                        <div>状态:</div>
                        <div>
                            <input id="selStatus" name="selStatus" type="text" class="easyui-combobox" style="width: 100px;" />
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
    <div data-options="region:'center',border:false">
        <table id="grid" data-options="fit:true,border:false"></table>
    </div>
</div>